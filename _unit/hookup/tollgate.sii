tollgate: tollgate.custom
{
	# actual tollgate type and toll value must be set in map

	model: "/model/activation/tollgate.pmd"
	animation: "/model/activation/tollgate.pma"

	trigger_distance: 3.0
	reset_distance: 5.0
}

